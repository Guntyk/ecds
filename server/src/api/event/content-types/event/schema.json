{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Calendar",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "Classification tournament",
        "Ranking tournament",
        "Open tournament",
        "National championship",
        "Cup",
        "ECDS Championship"
      ],
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "maxLength": 280
    },
    "organization": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organization.organization",
      "inversedBy": "events"
    },
    "organizer": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date"
    },
    "address": {
      "type": "string",
      "required": false
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "style": {
      "type": "enumeration",
      "enum": [
        "Ballroom",
        "Street",
        "Caribbean",
        "Other"
      ],
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "acceptRegistration": {
      "type": "boolean"
    }
  }
}
